GRANT EXECUTE_CATALOG_ROLE TO stream;
GRANT CONNECT TO stream;
GRANT CREATE SESSION TO stream;
GRANT SELECT ON SYS.V_$DATABASE TO stream;
GRANT SELECT ON SYS.V_$ARCHIVED_LOG TO stream;
GRANT SELECT ON SYS.V_$LOGMNR_CONTENTS TO stream;
GRANT EXECUTE ON DBMS_LOGMNR TO stream;
GRANT EXECUTE ON DBMS_LOGMNR_D TO stream;
GRANT SELECT ANY TRANSACTION TO stream;
GRANT SELECT ANY TABLE TO stream;
